# PDF配置
PDF_PATH=data/input/转 Word_数据结构卷一.pdf
OUTPUT_CSV=data/output/exam_analysis.csv

# 处理配置
MIN_CONTENT_LENGTH=10
MAX_CONTENT_LENGTH=800

# 日志配置
LOG_LEVEL=INFO

# OCR配置（可选）
